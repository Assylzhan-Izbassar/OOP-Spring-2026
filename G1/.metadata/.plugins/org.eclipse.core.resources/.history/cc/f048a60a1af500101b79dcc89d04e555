package test;

public class TestClass {

	public static void main(String[] args) {
		
		TestClass ts;
		
		int x = 5;
		int y = 2 * x;
		
		x = x++ + 5;
		x = ++x * 2 + 10;
		
		
		System.out.println(5 * y);
		
		System.out.println("Hello, Java!");
		System.out.print("Our first introduction to OOP!");
	}

}
