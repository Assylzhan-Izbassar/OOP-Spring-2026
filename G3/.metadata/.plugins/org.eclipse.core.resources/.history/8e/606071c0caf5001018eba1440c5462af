package test;

public class LoopArrayTest {

	public static void main(String[] args) {
		int[] arr = new int[10];
		
		for (int i=0; i < arr.length; ++i) {
			System.out.print(arr[i] + " ");
		}
		System.out.println();
		arr.
		
		int i=0;
		while(i < arr.length) {
			System.out.print(arr[i] + " ");
			i++;
		}
	}

}
